# CoachMic Backend Environment Variables
# Copy this file to .env for local development

# Application
APP_NAME=CoachMic
DEBUG=true
ENVIRONMENT=development

# Server
HOST=0.0.0.0
PORT=8000

# CORS (comma-separated origins)
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# Google Cloud Platform
GCP_PROJECT_ID=coachmic
GCP_LOCATION=us-central1
GCS_BUCKET_NAME=coachmic-uploads

# Vertex AI / Gemini - Default model used for all tasks unless overridden
GEMINI_MODEL=gemini-2.5-flash

# Default ElevenLabs API Key and Agent ID (can be overridden per user)
ELEVENLABS_API_KEY=your-elevenlabs-api-key
ELEVENLABS_AGENT_ID=your-agent-id

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60

# File Upload
MAX_UPLOAD_SIZE=5242880
